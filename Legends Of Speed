-- Define Library

local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/bloodball/-back-ups-for-libs/main/turtle"))()

-- Define Windows

local window1 = library:Window("Project 0n1")

local window2 = library:Window("More Farms")

local window3 = library:Window("Crystals")

local window4 = library:Window("Teleports")

local window5 = library:Window("More")

local window6 = library:Window("Discord")

-- Define Functions

function DestroyGui()

    game.CoreGui:FindFirstChild('TurtleUiLib'):Destroy()

end

-- Define Window1

window1:Label("Main Farms", Color3.fromRGB(127, 143, 166))

window1:Toggle("Auto Rebirths", false, function(state)

    	if state then           _G.loop = true

           while _G.loop == true do wait()

                 game:GetService("ReplicatedStorage").rEvents.rebirthEvent:FireServer("rebirthRequest")

           end

        else

           _G.loop = false

        end

end)

window1:Toggle("Auto Hoops", false, function(state)

    if state then

           _G.loop = true

           while _G.loop == true do wait()

                 local children = workspace.Hoops:GetChildren()

                    for i, child in ipairs(children) do

                    if child.Name == "Hoop" then

                          child.CFrame = game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame

                   end

                end

        end

        else

           _G.loop = false

        end

end)

